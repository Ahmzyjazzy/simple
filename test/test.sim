block showTokens(tokens) 
	printLine(true)
	println("  TOKEN GENERATED BY : "+compilerName)
	println("  VERSION : "+compilerVersion+"\tDATE : "+compilerDate)
	printLine(true)
	for a = 0 to len(tokens)
		println("  "+tokens[a].toString())
	loop
	printLine(false) __exit__
	
