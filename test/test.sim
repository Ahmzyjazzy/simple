class IterableAccessNode : Node

	block IterableAccessNode(location, itarget, iindex)
		sourceLocation = location 
		target = itarget
		index = iindex
		
	block visit(visitor)
		visitor.accept(this)
	
	block visitChildren(visitor)
		target.visit(visitor)
		index.visit(visitor)


