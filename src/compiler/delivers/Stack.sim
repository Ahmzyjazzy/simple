package simple.compiler.delivers

class Stack

	stackContent = []
	currentIndex = 0

	block push(ncontent)
		add(stackContent, ncontent)
		
	block pop()
		deleteItem(len(stackContent))
		
	block getCount()
		return len(stackContent) 