package simple.utilities

class Dictionary

	keys = []
	values = []

	block OldDictionary(ikey, ivalue)
		add(ikey, ivalue)
		
	block addValue(ikey, ivalue)
		add(keys, ikey) add(values, ivalue)
		
	block containsKey(key)
		for a=0 to len(keys) {
			if stringContains(keys[a], key) {
				return true
			}
		} return false
		
		