class TypeDefinition : Object

	typeDefinition = null
	typeName = "null"
	
	block TypeDefinition()
		init("typedef")
		
	block init(name)
	typeName = name
	addType(this)
	addAtribute("toString", toSimpleString(), 0)
		
	block toString()
		return typeName
		
	block toSimpleString(vm)
		return new SimpleString(typeName)
	