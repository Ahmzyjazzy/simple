package simple.parser.node

class EnumNode : Node

	attributes = null
	name = ""

	block EnumNode(location, iname)
		sourceLocation = location 
		attributes = new Dictionary
		name = iname
		
	block visit(visitor)
		visitor.accept(this)
	
	block visitChildren(visitor)
	
	block getAttributes()
		return attributes
		
	block getType()
		return "do_while_node"
		
	
	
