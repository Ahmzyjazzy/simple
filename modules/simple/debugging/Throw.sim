module simple.debugging

call "../core/loadcoredymodule.sim"
 
/*
	
*/
block throw(errorvalue)
	if errorvalue = "" {
		errorvalue = "Unknown Error Occur"
	}
	__throw(errorvalue)
	__exit__
	
	
	
	